{
    "headers": [
      {
        "source": "/(.*)",
        "dest":"/index.js",
        "methods":["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          },
          {
            "key": "Access-Control-Allow-Credentials",
            "value": "true"
          },
          {
            "key": "Access-Control-Allow-Headers",
            "value": "Origin, X-Requested-With, Content-Type, Accept"
          }
        ]
      }
    ]
  }
  